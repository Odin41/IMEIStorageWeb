<table id="imeis" class="table table-hover">
    <thead>
        <tr class="table-primary">
            <th scope="col">IMEI</th>
            <th scope="col">Дата добавления</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var imei in Model)
        {
            <tr>
                <td>@imei.Imei</td>
                <td>@imei.AddedDate.LocalDateTime.ToString("dd.MM.yyyy HH:mm:ss") </td>
            </tr>
        }
    </tbody>
</table>